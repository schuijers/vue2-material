{
  "name": "vue2-material",
  "version": "0.0.1",
  "description": "Material Design components for Vue 2.x",
  "main": "dist/vue2-material.js",
  "repository": {
    "url": "git@github.com:schuijers/vue2-material.git",
    "type": "git"
  },
  "author": "Martijn Schuijers",
  "license": "MIT",
  "dependencies": {
    "material-design-icons": "~3.0.1",
    "material-design-lite": "~1.2.1",
    "vue": "~2.0.5"
  },
  "devDependencies": {
    "autoprefixer": "~6.5.3",
    "babel-core": "~6.18.0",
    "babel-eslint": "~7.1.0",
    "babel-loader": "~6.2.7",
    "babel-plugin-istanbul": "~2.0.3",
    "babel-preset-es2015": "~6.18.0",
    "chai": "~3.5.0",
    "css-loader": "~0.25.0",
    "del-cli": "~0.2.0",
    "eslint": "~3.10.1",
    "eslint-config-vue": "~2.0.1",
    "eslint-loader": "~1.6.1",
    "eslint-plugin-html": "~1.6.0",
    "eslint-plugin-vue": "~1.0.0",
    "extract-text-webpack-plugin": "~v2.0.0-beta.4",
    "file-loader": "~0.9.0",
    "html-webpack-plugin": "~2.24.0",
    "istanbul-instrumenter-loader": "~1.0.0",
    "karma": "~1.3.0",
    "karma-chai": "~0.1.0",
    "karma-coverage": "~1.1.1",
    "karma-mocha": "~1.3.0",
    "karma-mocha-reporter": "~2.2.0",
    "karma-phantomjs-launcher": "~1.0.2",
    "karma-webpack": "~1.8.0",
    "mocha": "~3.1.2",
    "npm-run-all": "~3.1.1",
    "postcss-loader": "~1.1.1",
    "postcss-reporter": "~2.0.0",
    "stylelint": "~7.5.0",
    "stylelint-config-standard": "~14.0.0",
    "stylelint-processor-html": "~1.0.0",
    "vue-html-loader": "~1.2.3",
    "vue-loader": "~9.9.0",
    "vue-style-loader": "~1.0.0",
    "webpack": "~2.1.0-beta.26",
    "webpack-dev-server": "~2.1.0-beta.11",
    "webpack-merge": "~0.16.0"
  },
  "scripts": {
    "build": "npm-run-all build:dist",
    "build:dist": "webpack --env.dist",
    "prebuild:dist": "npm-run-all clean lint test",
    "build:prod": "webpack --env.prod",
    "prebuild:prod": "npm-run-all clean lint test",
    "clean": "del dist/*",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "stylelint demo-app/**/*.css demo-app/**/*.vue src/**/*.css src/**/*.vue",
    "lint:js": "eslint --ext .js,.vue config demo-app src",
    "serve": "webpack-dev-server --env.dev",
    "start": "npm-run-all serve",
    "test": "karma start",
    "test:local": "karma start --no-single-run"
  }
}
